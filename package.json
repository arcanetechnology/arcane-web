  "packageManager": "yarn@3.2.1",
  "author": "Joel D'Souza <joeldsouzax@gmail.com>",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "dev": "yarn workspaces foreach -p -i run dev",
    "build": "yarn workspaces foreach -i -t run build",
