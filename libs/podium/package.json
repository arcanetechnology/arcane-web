{
  "name": "@arcane-web/podium",
  "description": "make your astro app a podlet in podium",
  "packageManager": "yarn@3.2.1",
  "author": {
    "email": "joeldsouzax@gmail.com",
    "name": "Joel D'Souza"
  },
  "version": "0.0.1",
  "files": [
    "src",
    "!src/index.d.ts",
    "!src/tests",
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.mjs",
    "./server.js": "./dist/server.mjs",
    "./package.json": "./package.json"
  },
  "keywords": [
    "arcane-platform",
    "podium",
    "module federation",
    "ssr"
  ],
  "scripts": {
    "build": "./build.cjs && yarn run typecheck:emit",
    "typecheck": "yarn run -T tsc --noEmit",
    "typecheck:emit": "yarn run -T tsc --declaration --emitDeclarationOnly --outDir dist"
  }
}
