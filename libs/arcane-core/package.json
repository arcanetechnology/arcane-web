{
  "name": "@arcane-web/arcane-core",
  "packageManager": "yarn@3.2.0",
  "author": {
    "email": "joeldsouzax@gmail.com",
    "name": "Joel D'Souza"
  },
  "version": "0.0.1",
  "files": [
    "src",
    "!src/index.d.ts",
    "!src/tests",
    "dist"
  ],
  "main": "./dist/index.mjs",
  "exports": {
    ".": "./dist/index.mjs",
    "./components": "./dist/components/index.d.mts",
    "./layouts": "./dist/layouts/index.d.mts"
  },
  "keywords": [
    "bootstrapping",
    "sdk",
    "connector",
    "arcane-platform"
  ],
  "scripts": {
    "build": "./build.cjs && yarn run typecheck:emit",
    "typecheck": "yarn run -T tsc --noEmit",
    "typecheck:emit": "yarn run -T tsc --declaration --emitDeclarationOnly --outDir dist"
  }
}
