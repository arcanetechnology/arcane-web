---
import * as Layouts from '@arcane-web/arcane-layouts';
import * as Components from '@arcane-web/arcane-components';
---

<Layouts.DefaultLayout title="Invest">
  <Layouts.Header slot="header">
    <Components.Navigation slot="navigation" />
    <Components.Authentication
      onClick={(e) => console.log('helo')}
      client:visible
      slot="auth"
    />
  </Layouts.Header>
  <div slot="main">
    <section>
      <div class="container">
        <slot name="main" />
      </div>
    </section>
  </div>
</Layouts.DefaultLayout>
