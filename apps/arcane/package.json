{
  "name": "@arcane-web/arcane",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "main": "src/server/index.ts",
  "scripts": {
    "dev": "NODE_ENV=production tsx watch server/src/index.ts",
    "start": "NODE_ENV=production",
    "build:client": "vite build --outDir ../dist/client client --ssrManifest",
    "build:ssr": "vite build -c client/vite.config.ts --outDir dist/ssr --ssr client/src/entry.server.tsx; mv dist/ssr/entry.server.js dist/ssr/entry.server.cjs",
    "build:server": "tsc -p tsconfig.node.json",
    "build": "npm-run-all --parallel build:*"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.3",
    "npm-run-all": "^4.1.5",
    "tsx": "^3.7.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.9",
    "vite-plugin-solid": "^2.2.6"
  },
  "dependencies": {
    "devalue": "^2.0.1",
    "express": "^4.18.1",
    "express-winston": "^4.2.0",
    "solid-app-router": "^0.4.1",
    "solid-js": "^1.4.7",
    "solid-meta": "^0.27.5",
    "winston": "^3.8.1"
  }
}
